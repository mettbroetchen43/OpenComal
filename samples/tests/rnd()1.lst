       10 // RND() sample program
       20 //
       30 DIM count#(0:1)
       40 PAGE
       50 PRINT "Count number of heads and tails"
       60 PRINT "(Press system dependent ESCAPE key to end)"
       70 //
       80 WHILE TRUE DO
       90   a#:=RND(1)
      100   count#(a#):+1
      110   CURSOR 1,a#+4
      120   PRINT count#(a#);
      130 ENDWHILE
