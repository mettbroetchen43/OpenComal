       10 // LOG() sample program
       20 //
       30 PAGE
       40 PRINT "10-logarithm"
       50 FOR f:=0.050000 TO 4 STEP 0.050000 DO
       60   plot(f, LOG(f))
       70 ENDFOR
       80 //
       90 CURSOR 1,23
      100 PRINT "If the above looks like a 10-logarithm curve, it might be ok"
      110 //
      120 PROC plot(x, y)
      130   x#:=INT(x*20)
      140   y#:=11-6*y
      150   CURSOR x#,y#
      160   PRINT "*";
      170 ENDPROC
