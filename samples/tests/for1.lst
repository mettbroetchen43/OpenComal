      150 //
      160 PROC toe(van, tot, stap, title$)
      170   PRINT title$;"   ";
      180   FOR f:=van TO tot STEP stap DO PRINT f;" ";
      190   PRINT 
      200 ENDPROC
      210 //
      220 PROC downtoe(van, tot, stap, title$)
      230   PRINT title$;"   ";
      240   FOR f:=van DOWNTO tot STEP stap DO
      250     PRINT f;" ";
      260   ENDFOR
      270   PRINT 
      280 ENDPROC
